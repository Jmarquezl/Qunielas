<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewModels="clr-namespace:Quinieleros.ViewModels"
             x:Class="Quinieleros.Views.Rank"
             Title="Quinieleros">

    <ContentPage.BindingContext>
        <viewModels:RankViewModel/>
    </ContentPage.BindingContext>

    <Grid x:Name="grdMain" BackgroundColor="Black" Margin="0,0,0,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid x:Name="grdRank" Grid.Row="0" BackgroundColor="LightCyan" Margin="0,0,0,0">
            <Label>Rank</Label>
        </Grid>
        <Grid x:Name="grdMenu" Grid.Row="1" BackgroundColor="DimGray" Margin="0,0,0,0">
            <Label>Menu</Label>
        </Grid>

    </Grid>
</ContentPage>