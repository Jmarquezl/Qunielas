<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Quinieleros.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:viewModels="clr-namespace:Quinieleros.ViewModels"
    xmlns:local="clr-namespace:Quinieleros"
    xmlns:views="clr-namespace:Quinieleros.Views"
    Shell.FlyoutBehavior="Disabled">
    
    <!--<ShellContent
        Title="Bienvenido"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />-->
    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
    <ShellContent
        Title="Quinieleros"
        ContentTemplate="{DataTemplate views:Login}"
        Route="/Views/MainPage"/>
        <ShellContent
        Title="Rank"
        Icon="bets.png"
        ContentTemplate="{DataTemplate views:Rank}"
        Route="/Views/Rank" />
        <ShellContent
        Title="General"
        Icon="tablageneral.png"
        ContentTemplate="{DataTemplate views:General}"
        Route="/Views/General" />
    </FlyoutItem>
</Shell>
